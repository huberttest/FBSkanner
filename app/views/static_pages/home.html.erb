<div class= "container">
  <h2><%= link_to("Wrocław - pokoje wynajem", "https://www.facebook.com/groups/facebuylubuskie") %></h2>
  <h3>wyświetliło: <%= @getall.count %></h3>
  <table border="2" >
    <% @getall.each do |abc| %>
      <tr>
        <td>
          <%= abc.id %>
        </td>
        <td>
          <%= link_to("LINK", "#{abc.linktooffer}") %>
        </td>
        <td>
          <%= link_to image_tag("#{abc.userphoto}") %>
        </td>
        <td>
          <%= abc.username %>
        </td>
        <td>
          <%= link_to image_tag("#{abc.adsphoto}") %>
        </td>
        <td>
          <%=  abc.description1 ? abc.description1 : abc.description2 %>
        </td>
        <td>
          <%=  abc.updatedinfacebok.slice!(0..18).sub("T", " ") %>
        </td>
      </tr>
    <% end %>
  </table>
</div>
